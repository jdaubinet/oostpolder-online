{
    "name": "bw-oostpolder-techt-team/oostpolder-online",
    "type": "project",
    "description": "oostpolder-online",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/homestead": "^8.2",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "barryvdh/laravel-ide-helper": "^2.4",
        "codedungeon/phpunit-result-printer": "0.25.0",
        "friendsofphp/php-cs-fixer": "^2.11",
        "laravel/dusk": "^5.0",
        "laravel/homestead": "^8.0",
        "laravel/telescope": "^1.0",
        "nunomaduro/larastan": "^0.3.1",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.0",
        "wnx/laravel-stats": "^1.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
